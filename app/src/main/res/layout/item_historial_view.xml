<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayoutHistorial"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/borde_negro"
    android:layout_marginBottom="1dp">


    <TextView
        android:id="@+id/textViewHoraHistorial"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:width="2dp"
        android:background="@drawable/borde_negro"
        android:text="Hora"
        android:textColor="#000000"
        android:textSize="15dp"
        android:textStyle="bold" />/>

    <TextView
        android:id="@+id/textViewInfoHistorial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textViewHoraHistorial"
        android:layout_marginRight="15dp"
        android:text="Informacio"
        android:textColor="#000000"
        android:textSize="15dp" />


</RelativeLayout>